 {  "name": "Reporte de Garitas Tijuana",
 	"version": "1.0",   
 	"manifest_version":2,
 	"description": "Reporte de Garitas Tijuana - San Diego",        
	"permissions": [           
		"http://apps.cbp.gov/*",
		"http://weather.yahooapis.com/*",
		"tabs"  
		], 
	"icons": { "16": "images/icon16.png",
           	   "48": "images/icon48.png",
               "128": "images/icon128.png" }, 
	"browser_action": {     
		"default_icon": "images/icon.png",     
		"default_title": "Reporte de Garitas Tijuana - San Diego",     
		"default_popup": "popup.html"   
		} 
} 
